<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="94.0" prefHeight="84.0" prefWidth="147.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.Stats">
   <children>
      <Label fx:id="krzyzyk" alignment="CENTER" text="Gracz X: 0" />
      <Label fx:id="kolko" alignment="CENTER" text="Gracz O: 0" />
      <Button fx:id="single" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#newSingleGame" prefHeight="25.0" prefWidth="146.0" text="New Singleplayer Game" />
      <Button fx:id="multi" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#newMultiGame" prefHeight="25.0" prefWidth="146.0" text="New Multiplayer Game" />
   </children>
</VBox>
